{
  "private": true,
  "name": "blog",
  "description": "Minimal Blog @ Azure Static Web Apps",
  "version": "0.0.1",
  "author": "Takeharu Oshida",
  "license": "0BSD",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "NODE_ENV=production gatsby build",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "BABEL_ENV=test JEST_JUNIT_OUTPUT_DIR=test_results/junit/js-test-results.xml jest --testResultsProcessor='jest-junit' --collectCoverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-minimal-blog": "3.0.1",
    "badgen": "3.2.2",
    "gatsby": "3.8.0",
    "gatsby-plugin-manifest": "3.8.0",
    "gatsby-plugin-offline": "4.8.0",
    "gatsby-plugin-sitemap": "4.4.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "1.1.24",
    "react": "17.0.2",
    "react-icons": "4.2.0"
  },
  "devDependencies": {
    "babel-jest": "26.6.3",
    "babel-preset-env": "1.7.0",
    "babel-preset-gatsby": "1.8.0",
    "babel-preset-react": "6.24.1",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "jest-junit": "12.2.0",
    "lint-staged": "11.0.0",
    "node-fetch": "2.6.1",
    "prettier": "2.3.1",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "textlint": "12.0.0",
    "textlint-filter-rule-comments": "1.2.2",
    "textlint-rule-preset-ja-technical-writing": "6.0.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md,mdx}": "prettier --write",
    "*.ja.md": "textlint"
  }
}
