{
  "private": true,
  "name": "blog",
  "description": "Minimal Blog @ Azure Static Web Apps",
  "version": "0.0.1",
  "author": "Takeharu Oshida",
  "license": "0BSD",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "NODE_ENV=production gatsby build",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "pretest": "npx browserslist@latest --update-db",
    "test": "BABEL_ENV=test JEST_JUNIT_OUTPUT_DIR=test_results/junit/js-test-results.xml jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-minimal-blog": "3.1.3",
    "badgen": "3.2.2",
    "gatsby": "3.14.6",
    "gatsby-plugin-manifest": "3.14.0",
    "gatsby-plugin-offline": "4.14.0",
    "gatsby-plugin-sitemap": "4.10.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "1.1.27",
    "react": "18.1.0",
    "react-icons": "4.3.1"
  },
  "devDependencies": {
    "babel-jest": "27.5.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-gatsby": "1.14.0",
    "babel-preset-react": "6.24.1",
    "husky": "8.0.1",
    "jest": "27.5.1",
    "jest-junit": "13.2.0",
    "lint-staged": "12.4.1",
    "node-fetch": "2.6.7",
    "prettier": "2.6.2",
    "react-dom": "18.1.0",
    "react-test-renderer": "18.1.0",
    "textlint": "12.1.1",
    "textlint-filter-rule-comments": "1.2.2",
    "textlint-rule-preset-ja-technical-writing": "7.0.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md,mdx}": "prettier --write",
    "*.ja.md": "textlint"
  },
  "jest": {
    "testResultsProcessor": "jest-junit",
    "collectCoverage": true
  }
}
