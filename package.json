{
  "private": true,
  "name": "blog",
  "description": "Minimal Blog @ Azure Static Web Apps",
  "version": "0.0.1",
  "author": "Takeharu Oshida",
  "license": "0BSD",
  "starter-name": "gatsby-starter-minimal-blog",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "build": "NODE_ENV=production gatsby build",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "pretest": "npx browserslist@latest --update-db",
    "test": "BABEL_ENV=test JEST_JUNIT_OUTPUT_DIR=test_results/junit/js-test-results.xml jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-minimal-blog": "^6.0.5",
    "badgen": "3.2.2",
    "gatsby": "^5.5.0",
    "gatsby-plugin-feed": "^5.5.0",
    "gatsby-plugin-manifest": "^5.5.0",
    "gatsby-plugin-sitemap": "^6.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "4.3.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "babel-jest": "^29.5.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-gatsby": "^3.8.0",
    "cross-env": "^7.0.3",
    "husky": "8.0.3",
    "jest": "27.5.1",
    "jest-junit": "13.2.0",
    "lint-staged": "12.5.0",
    "node-fetch": "3.3.1",
    "prettier": "2.8.7",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "textlint": "12.1.1",
    "textlint-filter-rule-comments": "1.2.2",
    "textlint-rule-preset-ja-technical-writing": "7.0.0",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md,mdx}": "prettier --write",
    "*.ja.md": "textlint"
  },
  "jest": {
    "testResultsProcessor": "jest-junit",
    "collectCoverage": true
  }
}
